+          android:layout_marginStart="12dp"
+          android:text="-- km"
+          android:textColor="#666666"
+          android:textSize="16sp" />
+
+        <TextView
+          android:id="@+id/etaText"
+          android:layout_width="wrap_content"
+          android:layout_height="wrap_content"
